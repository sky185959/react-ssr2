{
  "name": "react-webpack-project",
  "version": "1.0.0",
  "description": "a react project build by webpack",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=develop webpack-dev-server --history-api-fallback  --config ./build/webpack.dev.conf.js",
    "build": "cross-env NODE_ENV=production webpack -p --config ./build/webpack.prod.conf.js",
    "analyse": "cross-env NODE_ENV=analyse webpack-dev-server --history-api-fallback --config ./build/webpack.dev.conf.js",
    "test": "webpack-dev-server --config ./test/webpack.conf.js"
  },
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wangerniu",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "cross-env": "^5.1.1",
    "chalk": "^2.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^3.0.0-alpha.3",
    "less-loader": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.7.1",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.2",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-css-modules": "^4.7.1",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2"
  }
}
